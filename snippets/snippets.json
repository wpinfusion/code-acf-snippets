  "acf_add_field_group": {
    "prefix": "acf_add_field_group",
    "body": "acf_add_local_field_group([
        'key'                   => '${1: group_KEY}',
        'title'                 => '${2: TITLE}',
        'fields'                => [],
        'location'              => [
            [
              [
                'param'    => 'post_type',
                'operator' => ' == ',
                'value'    => 'post',
              ],
            ],
        ],
        'menu_order'            => 0,
        'position'              => 'normal',
        'style'                 => 'default',
        'label_placement'       => 'top',
        'instruction_placement' => 'label',
        'hide_on_screen'        => '',
    ]),$0
    ",
    "description": "Adds an ACF Field Group",
    "scope": "source.php"
  },
